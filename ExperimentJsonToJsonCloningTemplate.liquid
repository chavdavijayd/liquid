          "col{{forloop.index}}": "{{vjcol}}"
          {%- if forloop.last == false -%}
              ,
          {%- endif -%}
        {%- endfor -%}
        }
        {%- if forloop.last == false -%}
              ,
        {%- endif -%}
   {%- endfor -%}
} {% endcomment %}